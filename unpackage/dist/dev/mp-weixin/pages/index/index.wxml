<view class="container"><swiper class="swiper-banner" circular="{{true}}" autoplay="{{true}}" interval="3000"><block wx:for="{{imgList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image class="img" src="{{item.img}}"></image></swiper-item></block></swiper><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['listData','',index]]]]]]]}}" bindtap="__e" class="_div"><view class="_div">{{item.title}}</view><image src="{{item.cover}}"></image></view></block><button data-event-opts="{{[['tap',[['toHome',['$event']]]]]}}" bindtap="__e">tohome</button><button data-event-opts="{{[['tap',[['toMessage',['$event']]]]]}}" bindtap="__e">to message</button></view>